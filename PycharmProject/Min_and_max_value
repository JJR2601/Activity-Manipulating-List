grades = [75, 80, 85, 90, 95, 100]

def maxandmin(grades):
    max_grades = 0
    for grade in grades:
        if grade > max_grades:
            max_grades = grade
    return max_grades

# Custom function
print(max(grades))

# Built in function
print("Max grades: ", max(grades))
print("Min grades: ", min(grades))
